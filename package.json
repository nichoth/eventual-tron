  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^5.1.0",
    "electron": "^9.4.0",
    "node-sass": "^4.13.1"
  }
